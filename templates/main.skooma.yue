import output from require 'params'

(...) =>
	head_content, body_content = @(...)
	body_content = head_content unless body_content
	render.html(html {
		lang: "english"
		head {
			link rel: "stylesheet", href: "/#{output}/css/site.css"
			meta charset: "UTF-8"
			head_content
		}
		body {
			body_content
		}
	})
